<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:Master="clr-namespace:TiposPagina.TipoPagina.MasterDetails"
             x:Class="TiposPagina.TipoPagina.MasterDetails.Master">
   
   
    <MasterDetailPage.Master >
         <!--Menu-->
        <ContentPage Title="Menu" BackgroundColor="Red">
            <StackLayout>
                <Button Text="Pagina1" BackgroundColor="Transparent" Clicked="MudarPagina1"/>
                <Button Text="Pagina2" BackgroundColor="Transparent"  Clicked="MudarPagina2"/>
                <Button Text="Conteudo" BackgroundColor="Transparent"  Clicked="MudarConteudo"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
    
    
    <MasterDetailPage.Detail>
        <!--Conteudo-->
        <Master:Conteudo/>

    </MasterDetailPage.Detail>
 
</MasterDetailPage>