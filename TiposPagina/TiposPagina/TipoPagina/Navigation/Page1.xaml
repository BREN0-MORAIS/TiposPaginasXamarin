            <Button Text="for page two"  Clicked="MudarPageTwo"/>

            <Button Text="ChamarModal" Clicked="ChamarModal"/>
            <Button Text="ChamarMaster" Clicked="ChamarMaster"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>